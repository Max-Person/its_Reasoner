@base <http://www.vstu.ru/poas/code> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Arr[B + C] - выбран []

# +++++++++++++++++++++++++++++++++ Элементы +++++++++++++++++++++++++++++++++
# ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

<#element_Arr_1>
    a <#operand> ;
    <#state> <#evaluated> ;
    <#has> <#token_Arr_1> ;
    <#RU_localizedName> "операнд Arr на позиции 1"^^xsd:string .

<#element_brackets_1>
    a <#brackets> ;
    <#state> <#unevaluated> ;
    <#has> <#token_brackets_1> ;
    <#has> <#token_brackets_2> ;
    <#var...> "X"^^xsd:string ;
    <#RU_localizedName> "оператор [] после Arr"^^xsd:string .

<#element_B_1>
    a <#operand> ;
    <#state> <#evaluated> ;
    <#has> <#token_B_1> ;
    <#isInside> <#element_brackets_1> ;
    <#RU_localizedName> "операнд B на позиции 3"^^xsd:string .

<#element_plus_1>
    a <#plus> ;
    <#state> <#unevaluated> ;
    <#has> <#token_plus_1> ;
    <#isInside> <#element_brackets_1> ;
    <#RU_localizedName> "оператор + на позиции 4"^^xsd:string .

<#element_C_1>
    a <#operand> ;
    <#state> <#evaluated> ;
    <#has> <#token_C_1> ;
    <#isInside> <#element_brackets_1> ;
    <#RU_localizedName> "операнд C на позиции 5"^^xsd:string .

# ++++++++++++++++++++++++++++++++++ Токены ++++++++++++++++++++++++++++++++++
# ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

<#token_Arr_1>
    a <#token> ;
    <#directlyLeftOf> <#token_brackets_1> ;
    <#belongsTo> <#element_Arr_1> ;
    <#RU_localizedName> "Arr на позиции 1"^^xsd:string .

<#token_brackets_1>
    a <#token> ;
    <#directlyLeftOf> <#token_B_1> ;
    <#belongsTo> <#element_brackets_1> ;
    <#var...> "X1"^^xsd:string ;
    <#RU_localizedName> "[ на позиции 2"^^xsd:string .

<#token_B_1>
    a <#token> ;
    <#directlyLeftOf> <#token_plus_1> ;
    <#belongsTo> <#element_B_1> ;
    <#RU_localizedName> "B на позиции 3"^^xsd:string .

<#token_plus_1>
    a <#token> ;
    <#directlyLeftOf> <#token_C_1> ;
    <#belongsTo> <#element_plus_1> ;
    <#RU_localizedName> "+ на позиции 4"^^xsd:string .

<#token_C_1>
    a <#token> ;
    <#directlyLeftOf> <#token_brackets_2> ;
    <#belongsTo> <#element_C_1> ;
    <#RU_localizedName> "C на позиции 5"^^xsd:string .

<#token_brackets_2>
    a <#token> ;
    <#belongsTo> <#element_brackets_1> ;
    <#var...> "X2"^^xsd:string ;
    <#RU_localizedName> "] на позиции 6"^^xsd:string .
